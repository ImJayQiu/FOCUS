	<div class="col-md-3">
	    <div class="course-box shop-wrapper">

<table class=table >

    <tr>
	<td align=center colspan=2 >
	    <h1>Forecast Result of <%= current_user.country %></h1>
	</td>
    </tr>


    <% @all_output.each do |final|  %> 


	<tr>
	    <td align=center colspan=4 ><%= final[:method]  %></td>
	</tr>

	<tr>

	    <% final[:png].each do |png|  %> 

		<% png_name =  png.split("/")[-1] %>

		<td align=center ><%= image_tag  root_url + @download_dir + png_name  %></td>

	    <% end %><!-- @final_png -->

	</tr>



	<tr>
	    <% final[:nc].each do |nc|  %> 
		<% file_name =  nc.split("/")[-1] %>

		<td align=center colspan=2 ><%=file_name[0...-3]%><br/><%= link_to "Download NC", root_url + @download_dir + file_name %></td>
	    <% end %><!-- final[:nc_file] -->

	</tr>


    <% end %><!-- @final_output -->

</table>
</div>
</div>

<table class=table >

    <tr>

	<td align=right ><%= p "File Name : " %></td>

	<td align=left ><%= link_to "Params.txt", root_url + @download_dir + "params.txt" %></td>

    </tr>

</table>


